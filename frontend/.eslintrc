{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "linebreak-style": 0,
    "strict": ["error", "global"],
    "comma-dangle": ["warn", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "functions": "ignore",
    }],
    no-unused-vars: ["error", { "argsIgnorePattern": "^_" }]
  }
}
